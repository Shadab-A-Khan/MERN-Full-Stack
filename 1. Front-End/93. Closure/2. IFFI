___________________________script1____________________________


(function print1(){
    var name = 'file1';

 setTimeout(function(){
    console.log(name);
 },1000);
})();

// print1();


__________________________script2____________________________

(function print2(){
    var name='file2';
    console.log(name);
})();

//print2();


                      -----------
                      ouput:
                      file2
                      file1
                      -----------







